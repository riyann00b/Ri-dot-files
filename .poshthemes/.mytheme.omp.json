{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  
  // Console Title
  "console_title_template": "{{ if .Root }}root @ {{ end }}{{ .Shell }} in {{ .Folder }}{{ if .Segments.Python.Venv }} Conda Py: {{ .Segments.Python.Full }} Env: {{ .Segments.Python.Venv }}{{ end }}{{ if .Segments.Python.Venv == 'mamba' }} Mamba: {{ .Segments.Python.Full }} Env: {{ .Segments.Python.Venv }}{{ end }}",  // Prompt Blocks
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#c386f1",
          "leading_diamond": "ÓÇ∂",
          //"trailing_diamond": "ÓÇ∞",
          "template": "{{ if .SSHSession }}ÓÆ© {{ end }}{{ .UserName }}"
        },
        {
          // OS Segment
          "type": "os",
          "style": "diamond",
          "background": "#575656",
          "foreground": "#D6DEEB",
          "leading_diamond": "\ue0b2",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }} ",
          "properties": {
            "alpine": "\uf300",
            "arch": "ÔåÉ",
            "centos": "\uf304",
            "debian": "ÓØÖ",
            "elementary": "\uf309",
            "fedora": "ÓΩÜ",
            "gentoo": "\uf30d",
            "linux": "ÓØÜ",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "ÓØâ",
            "kalilinux": "Ôåß",
            "windows": "Óò™"
          }
        },
        {
          //project
          "type": "project",
          "style": "powerline",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "powerline_symbol": "\ue0b0",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}Ôíá {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} ",
          "properties": {
            "always_enabled":true
          }
        },
        {
          // NPM
          "type": "npm",
          "style": "powerline",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "powerline_symbol": "ÓÇ∞",
          "template": "Óúû {{ .Full }} ",
          "properties": {
            "home_enabled": false,
            "fetch_version": true,
            "missing_command_text": "",
            "display_mode": "context",
            "version_url_template": "",
            "extensions": ["package.json", "package-lock.json"],
            "folders": [],
            "cache_version": false
          }
        },      
        {
          // Shell Segment
          "type": "shell",
          "style": "powerline",
          "background": "#33ff52",
          "foreground": "#011627",
          "powerline_symbol": "\ue0b0",
          "template": " \uf489 {{ .Name }} "
        },
        {
          // Root Segment
          "type": "root",
          "style": "powerline",
          "background": "#EF541C",
          "foreground": "#D6DEEB",
          "powerline_symbol": "\ue0b0",
          "template": " Ó≠¥ admin "
        },
        {
          // React
          "type": "react",
          "style": "powerline",
          "foreground": "#81a1c1",
          "powerline_symbol": "\ue0b0",
          "template": " Óû∫ {{ .Full }} ",
          "properties": {
            "home_enabled": false,
            "fetch_version": true,
            "missing_command_text": "",
            "display_mode": "context",
            "version_url_template": "",
            "extensions": ["package.json"],
            "folders": [],
            "cache_version": false
          }
        },
        {
          // Terraform Context
          "type": "terraform",
          "style": "powerline",
          "foreground": "#000000",
          "background": "#ebcc34",
          "powerline_symbol": "\ue0b0",
          "template": "{{.WorkspaceName}}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "aws",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#FFA400",
          "template": " Óû≠ {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "properties": {
            "display_default": true
          }
        },
        {
          "type": "az",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#000000",
          "background": "#9ec3f0",
          "template": " ÓØò {{ .EnvironmentName }}",
          "properties": {
            "source": "pwsh"
          }
        },
        {
          "type": "azd",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#000000",
          "background": "#9ec3f0",
          "template": " ÓØò {{ .DefaultEnvironment }} "
        },
        {
          "type": "azfunc",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#FEAC19",
          "template": " ÔÉß {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "display_mode": "files",
            "home_enabled": false,
            "missing_command_text": "",
            "version_url_template": "",
            "extensions": ["host.json", "local.settings.json", "function.json"],
            "folders": [],
            "cache_version": false
          }
        },        
        {
          // CMake Segment
          "type": "cmake",
          "style": "powerline",
          "background": "#1E9748",
          "foreground": "#E8EAEE",
          "powerline_symbol": "\ue0b0",
          "template": " ÓØÑ \ue61d cmake {{ .Full }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#306998",
          "background": "#ffd43b",
          "powerline_symbol": "\ue0b0",
          "template": " Óàµ {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "home_enabled": true,
            "fetch_virtual_env": true,
            "display_default": false,
            "fetch_version": false,
            "missing_command_text": "",
            "display_mode": "context",
            "version_url_template": "",
            "extensions": ["*.py", "*.ipynb", "pyproject.toml", "venv.bak"],
            "folders": [],
            "cache_version": false,
            "folder_name_fallback": true
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "template": " Óú∏ {{ .Full }}",
          "properties": {
            "home_enabled": true,
            "fetch_version": true,
            "missing_command_text": "",
            "display_mode": "context",
            "version_url_template": "",
            "extensions": ["*.java", "*.class", "*.gradle", "*.jar", "*.clj", "*.cljr", "pom.xml", "build.gradle.kts", "build.sbt", ".java-version", ".deps.edn", "project.clj", "build.boot"],
            "folders": [],
            "cache_version": false
          }
        },        
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#303030",
          "background": "#3c873a",
          "template": " Ó¥ç {{ .Full }} ",
          "properties": {
            "home_enabled": true,
            "fetch_version": true,
            "missing_command_text": "",
            "display_mode": "context",
            "version_url_template": "",
            "fetch_package_manager": true,
            "pnpm_icon": "\uF02C1",
            "yarn_icon": "\uF011B",
            "npm_icon": "\uE71E",
            "extensions": ["*.js", "*.ts", "package.json", ".nvmrc", "pnpm-workspace.yaml", ".pnpmfile.cjs", ".vue"],
            "folders": [],
            "cache_version": false
          }
        },
        {
          "type": "kotlin",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#906cff",
          "template": " <b>K</b> {{ .Full }} ",
          "properties": {
            "home_enabled": true,
            "fetch_version": true,
            "missing_command_text": "",
            "display_mode": "context",
            "version_url_template": "",
            "extensions": ["*.kt", "*.kts", "*.ktm"],
            "folders": [],
            "cache_version": false
          }
        },
        {
          "type": "lua",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "white",
          "background": "blue",
          "template": " Óò† {{ .Full }} ",
          "properties": {
            "home_enabled": true,
            "fetch_version": true,
            "missing_command_text": "",
            "display_mode": "context",
            "version_url_template": "",
            "preferred_executable": "lua",
            "extensions": ["*.lua", "*.rockspec"],
            "folders": [],
            "cache_version": false
          }
        },
              
        {
          // Go Segment
          "type": "go",
          "style": "powerline",
          "background": "#7FD5EA",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "template": " \u202d\ue626 {{ .Full }} "
        },
        {
          //Docker Segment
          "type": "docker",
          "style": "powerline",
          "foreground": "#000000",
          "background": "#0B59E7",
          "powerline_symbol": "\ue0b0",
          "template": " Ôåà {{ .Context }} "
        },
        {
          //firebase
          "type": "firebase",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#FFA000",
          "powerline_symbol": "\ue0b0",
          "template": " Û∞•ß {{ .Project }}"
        },
        {
          // Flutter Segment
          "type": "flutter",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#06A4CE",
          "powerline_symbol": "\ue0b0",
          "template": " Óäé {{ .Full }} "
        },
        {
          // Rust Segment
          "type": "rust",
          "style": "powerline",
          "background": "#99908A",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "template": "{{ if .Full }} \ue7a8 {{ .Full }} {{ end }} "
        },
        {
          // Git Segment
          "type": "git",
          "style": "powerline",
          "foreground": "#000000",
          "background": "#3e2c00",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#CAEBE1> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#16B1AC{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#16B1AC{{ end }}",
            "{{ if gt .Ahead 0 }}#B787D7{{ end }}",
            "{{ if gt .Behind 0 }}#B787D7{{ end }}"
          ]
        },
        {
          //gitversion
          "type": "gitversion",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#3e2c00",
          "template": " Ôáí {{ .MajorMinorPatch }} "
        },
        {
          "type": "spotify",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#1BD760",
          "template": "{{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }}",
          "properties": {
            "playing_icon": "ÓòÇ ",
            "paused_icon": "Ô££ ",
            "stopped_icon": "ÔÅç "
          }
        },
        {
          "type": "ytm",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#FF0000",
          "template": "{{ .Icon }}{{ if ne .Status \\\"stopped\\\" }}{{ .Artist }} - {{ .Track }}{{ end }}",
          "properties": {
            "playing_icon": "ÓòÇ ",
            "paused_icon": "Ô££ ",
            "stopped_icon": "ÔÅç ",
            "api_url": "http://127.0.0.1:13091"
          }
        },

        {
          //bun
          "type": "bun",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "background": "#F6F6F6",
          "foreground": "#3C82F6",
          "template": " ü•ü {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "home_enabled": false,
            "fetch_version": true,
            "missing_command_text": "",
            "display_mode": "context",
            "version_url_template": "",
            "extensions": ["bun.lockb"],
            "folders": [],
            "cache_version": false
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          // Status Segment
          "type": "status",
          "style": "diamond",
          "background": "#03DED3",
          "foreground": "#414141",
          "leading_diamond": "\ue0b6",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "properties": {
            "always_enabled": true
          },
          "background_templates": [
            "{{ if gt .Code 0 }}#E44141{{ end }}"
          ],
          "foreground_templates": [
            "{{ if gt .Code 0 }}#D6DEEB{{ end }}"
          ]
        },
        {
          "type": "nightscout",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#ff0000",
          "background_templates": [
            "{{ if gt .Sgv 150 }}#FFFF00{{ end }}",
            "{{ if lt .Sgv 60 }}#FF0000{{ end }}",
            "#00FF00"
          ],
          "foreground_templates": [
            "{{ if gt .Sgv 150 }}#000000{{ end }}",
            "{{ if lt .Sgv 60 }}#000000{{ end }}",
            "#000000"
          ],
          "leading_diamond": "ÓÇ∂",
          "trailing_diamond": "ÓÇ∞",
          "template": "Óä° {{ .Sgv }}{{ .TrendIcon }}",
          "properties": {
            "url": "https://YOURNIGHTSCOUTAPP.herokuapp.com/api/v1/entries.json?count=1&token=APITOKENFROMYOURADMIN",
            "http_timeout": 1500
          }
        },        
        {
          "type": "sysinfo",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#8f43f3",
          "template": " Óâ¶ {{ round .PhysicalPercentUsed .Precision }}% ",
          "properties": {
            "precision": 2
          },
          "style": "powerline"
        },         
        {
          // Execution Time Segment
          "type": "executiontime",
          "style": "diamond",
          "background": "#575656",
          "foreground": "#D6DEEB",
          "template": " \uf252 {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "threshold": 0
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          // Text Segment (Start of New Line)
          "type": "text",
          "style": "plain",
          "foreground": "#D6DEEB",
          "template": "\u256d\u2500"
        },
        {
          // Time Segment
          "type": "time",
          "style": "plain",
          "foreground": "#F2D3B6",
          "template": "{{ .CurrentDate | date .Format }} <#79DFE1>|</>",
          "properties": {
            "time_format": "<#D6DEEB>\ue641 03:04:05 PM</> <#79DFE1>|</> \uf073 2 Jan, Monday"
          }
        },
        {
          // Path Segment
          "type": "path",
          "style": "diamond",
          "foreground": "#B6D6F2",
          "leading_diamond": "<#00C7FC> \uf07b </><#B6D6F2> in </>",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": " \uf07c ",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "\ueb06 ",
            "style": "agnoster_short",
            "max_depth": 3
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          // Text Segment (End of New Line)
          "type": "text",
          "style": "plain",
          "foreground": "#D6DEEB",
          "template": "\u2570\u2500"
        },
          
        {
          // Status Segment
          "type": "status",
          "style": "plain",
          "foreground": "#D6DEEB",
          "template": "\u276f ",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    }
  ],
  
  // Additional Configuration
  "osc99": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "\ue285 "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#D6DEEB",
    "template": "\u2570\u2500\u276f "
  },
  "version": 2
}